# Server environment
PORT=4000
DATABASE_URL=postgres://postgres:postgres@localhost:5432/emergency_help_anywhere
JWT_SECRET=replace-with-strong-secret
CORS_ORIGIN=http://localhost:5173

# Twilio (optional, for SMS/voice fallback)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_FROM_NUMBER=+1234567890
# Default destination for emergency SMS/calls (e.g. dispatcher or test phone)
EMERGENCY_FALLBACK_TO_NUMBER=+1234567890

# OpenAI (optional, for AI emergency classification)
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4o-mini
